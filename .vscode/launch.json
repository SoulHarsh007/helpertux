{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": [
        "--max-old-space-size=400",
        "--expose_gc",
        "--gc-interval=100",
      ],
      "program": "${workspaceFolder}/TuxManager.js",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
