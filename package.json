{
  "name": "helpertux",
  "version": "1.0.0-Beta",
  "main": "index.js",
  "author": "SoulHarsh007",
  "license": "BSD-3-Clause",
  "type": "module",
  "cacheDirectories": [
    ".cache",
    "node_modules"
  ],
  "scripts": {
    "start": "node --enable-one-shot-optimization --max-old-space-size=400 --expose-gc --gc-interval=100 --clear-free-memory TuxManager.js",
    "lint": "eslint ./*.js ./commands/*.js ./utils/*.js ./events/*.js ./structures/*/*.js"
  },
  "engines": {
    "node": "15.x.x",
    "yarn": "1.x.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "devDependencies": {
    "eslint": "7.24.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.1.3",
    "node-gyp": "7.1.2",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "@nia3208/centra": "1.0.9-1",
    "@nia3208/pretty-ms": "7.0.0",
    "bufferutil": "4.0.3",
    "cheerio": "1.0.0-rc.5",
    "discord.js": "12.5.1",
    "erlpack": "discord/erlpack",
    "fastify": "3.14.0",
    "pretty-bytes": "5.6.0",
    "utf-8-validate": "5.0.4",
    "zlib-sync": "0.1.7"
  }
}
