{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "google",
    "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:jsdoc/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {"jsdoc": {"mode": "typescript"}},
  "rules": {
    "prettier/prettier": "warn",
    "valid-jsdoc": "off"
  }
}
